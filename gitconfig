[core]
  excludesFile = ~/.gitignore
[alias]
  a  = add
  b  = branch
  c  = commit
  d  = diff
  f  = fetch
  l  = log
  m  = merge
  p  = push
  s  = status
  t  = tag
  r  = remote
  br = browser
  cf = config
  cl = clone
  cr = create
  co = checkout
  fr = fork
  re = remote
  rs = reset
  rb = rebase
  rv = revert
  pl = pull
  st = stash
  ui = update-index
  ls-subtrees = !"git log | grep git-subtree-dir | tr -d ' ' | cut -d ":" -f2 | sort | uniq"

  hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
  type = cat-file -t
  dump = cat-file -p
[push]
	default = current
